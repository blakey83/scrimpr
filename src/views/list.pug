                  a(href='#')
                    img.img-fluid(src='/images/product/size-small/product-home-1-img-3.jpg' alt='')
                td.product-name
                  a(href='#') Product Name
                td.product-price-list
                  span.amount $00.00
                td.product-quantities
                  .quantity.d-inline-block
                    input(type='number' min='1' step='1' value='1')
